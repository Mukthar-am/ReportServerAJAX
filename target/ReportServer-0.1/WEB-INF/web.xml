<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>

	<servlet>
		<servlet-name>ActionServlet</servlet-name>
		<servlet-class>com.reports.ActionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>ResultsParser</servlet-name>
		<servlet-class>com.reports.ResultsParser</servlet-class>
	</servlet>
	
	<servlet-mapping>
		<servlet-name>ActionServlet</servlet-name>
		<url-pattern>/ActionServlet/*</url-pattern>
	</servlet-mapping>

	<servlet-mapping>
		<servlet-name>ResultsParser</servlet-name>
		<url-pattern>/adformatresults/*</url-pattern>
	</servlet-mapping>

</web-app>